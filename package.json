{
  "name": "chronos",
  "productName": "Chronos",
  "version": "7.0.0",
  "description": "Chronos Microservice Debugger",
  "main": "./build/Main.js",
  "scripts": {
    "build": "webpack",
    "start": "webpack serve",
    "nodemon": "nodemon server/server.js",
    "electron": "electron .",
    "electronTest": "cross-env NODE_ENV=development electron .",
    "electron:wsl": "electron . --no-sandbox",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:electron": "jest --detectOpenHandles ./__tests__/electron/*",
    "test:app": "jest ./__tests__/app/*",
    "both": "npm run start & sleep 5 && npm run electronTest",
    "both:wsl": "npm run start & sleep 5 && npm run electron:wsl",
    "both:tsc": "nodemon --watch ./app & npm run start & sleep 5 && npm run electron",
    "both:dev": "npm run start & nodemon --watch ./electron --exec 'npm run electron'",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-any": "electron-packager ./dist chronos --overwrite --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/chronos-darwin-x64/chronos.app/ Chronos",
    "delete": "rm -rf dist & rm -rf build",
    "installDep": "cd dist && npm i",
    "deleteDist": "node nodeUtilities/deleteDist.js",
    "reinstall": "rm -rf node_modules && npm install",
    "prepare": "tsc -v && tsc -p . && npm run build",
    "prepareDist": "npm run deleteDist && tsc -v && tsc -p . && npm run build && npm run copyAssetsDist && npm run copyPackageJsonDist && npm run copyBuildDist && npm run copyBuildDBDist && npm run copyBuildModelsDist && npm run copyBuildRoutesDist && npm run copyBuildUtilitiesDist && npm run copySettingsDist && npm run installDep",
    "rebuild": "npm run delete && npm run prepare",
    "copyAssetsDist": "echo D | xcopy app\\assets dist\\assets",
    "copyPackageJsonDist": "echo F | xcopy dist.package.json dist\\package.json",
    "copyBuildDist": "echo D | xcopy build dist\\build",
    "copyBuildDBDist": "echo D | xcopy build\\databases dist\\build\\databases",
    "copyBuildModelsDist": "echo D | xcopy build\\models dist\\build\\models",
    "copyBuildRoutesDist": "echo D | xcopy build\\routes dist\\build\\routes",
    "copyBuildUserDist": "echo D | xcopy build\\user dist\\build\\user",
    "copyBuildUtilitiesDist": "echo D | xcopy build\\utilities dist\\build\\utilities",
    "copySettingsDist": "echo F | xcopy settings.json dist\\settings.json"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "keywords": [
    "distributed tracing",
    "tracing"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",

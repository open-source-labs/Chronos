        "@material-ui/core": "^4.11.3",
        "@material-ui/icons": "^4.11.2",
        "@pionerlabs/grpc-interceptors": "^0.2.5",
        "@types/react-table": "^7.0.29",
        "chai": "^4.3.4",
        "chronos-tracker": "^6.0.3",
        "d3": "^6.7.0",
        "electron": "^12.0.5",
        "enzyme-adapter-react-16": "^1.15.6",
        "express": "^4.17.1",
        "moment": "^2.29.1",
        "mongodb": "^3.6.6",
        "mongoose": "^5.12.5",
        "pg": "^8.6.0",
        "plotly.js": "^1.58.4",
        "plotly.js-dist": "^1.58.4",

{
    "name": "chronos",
    "version": "6.1.0",
    "description": "Chronos Microservice Debugger",
    "main": "./build/Main.js",
    "scripts": {
        "build": "webpack",
        "start": "webpack serve",
        "nodemon": "nodemon server/server.js",
        "electron": "electron .",
        "electron:wsl": "electron . --no-sandbox",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:electron": "jest --detectOpenHandles ./__tests__/electron/*",
        "test:app": "jest ./__tests__/app/*",
        "both": "npm run start & sleep 5 && npm run electron",
        "both:wsl": "npm run start & sleep 5 && npm run electron:wsl",
        "both:tsc": "nodemon --watch ./app & npm run start & sleep 5 && npm run electron",
        "both:dev": "npm run start & nodemon --watch ./electron --exec 'npm run electron'",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
        "create-installer-mac": "electron-installer-dmg ./release-builds/chronos-darwin-x64/chronos.app/ Chronos",
        "delete": "rm -rf dist & rm -rf build",
        "reinstall": "rm -rf node_modules && npm install",
        "prepare": "tsc -v && tsc -p . && npm run build",
        "rebuild": "npm run delete && npm run prepare"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    },
    "keywords": [
        "distributed tracing",
        "tracing"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.35",
        "@fortawesome/free-solid-svg-icons": "^5.15.3",
        "@fortawesome/react-fontawesome": "^0.1.14",
